<%- include('../partials/header') %>

  <body class="bg-dark">
    <br><br>
    <div class="container d-flex flex-wrap gap-4"> 
    <% songs.forEach(elem => { %>
      <div class="row">
        <div class="col-sm-4">
          <div class="card shadow " style="width: 18rem;">
            <% if (elem.coverArt) { %>
              <img 
              src="<%= elem.coverArt %>" 
              class="card-img-top h-50" 
              alt="song's cover art" 
              >
            <% } else { %>
            <img src="#" class="card-img-top " alt="song's cover art">
            <% } %>
            <div class="card-body">
              <h5 class="card-title"><%= elem.artistAndName %></h5>
              <p class="card-text"><%= elem.genre %></p>
              <a href="/ysbpsongs/<%= elem._id %>" class="btn btn-primary">YSBP Journal Entries</a>
            </div>
          </div>
        </div>
      </div>
    <% }) %>
    </div>
  </body>
</html>

<%- include('../partials/footer') %>